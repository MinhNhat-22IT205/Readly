{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@features/*": [
        "src/features/*"
      ],
      "@shared-constants/*": [
        "src/shared/constants/*"
      ],
      "@shared-libs/*": [
        "src/shared/libs/*"
      ],
      "@shared-types/*": [
        "src/shared/types/*"
      ],
      "@shared-utils/*": [
        "src/shared/utils/*"
      ],
      "@shared-components/*": [
        "src/shared/components/*"
      ]
    }
  },
  "include": [
    "src",
    "app",
    "index.ts",
    "index.tsx",
    "nativewind-env.d.ts"
, "App.tsx"  ]
}